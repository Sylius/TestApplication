{
    "name": "sylius/test-kit",
    "require": {
        "php": "^8.2",
        "sylius/sylius": "^2.0",
        "symfony/debug-bundle": "*",
        "symfony/dotenv": "*",
        "symfony/flex": "*",
        "symfony/web-profiler-bundle": "*",
        "theofidry/alice-data-fixtures": "*",
        "symfony/runtime": "*"
    },
    "bin": [
        "bin/console"
    ],
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "TestKitApp\\": "src/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
